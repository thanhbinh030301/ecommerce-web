{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard-with-typescript",
    "react-app",
    "plugin:import/recommended",
    "plugin:import/warnings"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/order": [
      "off",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
